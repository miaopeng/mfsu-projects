{
  "name": "hawkeye",
  "version": "1.0.0",
  "private": true,
  "description": "Guanyun CRM",
  "scripts": {
    "analyze": "cross-env ANALYZE=1 umi build",
    "build": "umi build",
    "dev": "npm run start:dev",
    "deploy": "cd .. && bundle exec mina deploy",
    "lint": "umi g tmp && npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier --check \"**/*\" --end-of-line auto",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "prettier": "prettier -c --write \"**/*\"",
    "puma:start": "cd .. && bundle exec mina puma:start",
    "puma:stop": "cd .. && bundle exec mina puma:stop",
    "mock": "cd mock_apollo && nodemon apollo_server.js",
    "start": "cross-env UMI_UI=none umi dev",
    "start:no-mock": "cross-env MOCK=none umi dev",
    "start:no-ui": "cross-env UMI_UI=none umi dev",
    "start:dev": "cross-env REACT_APP_ENV=dev MOCK=none UMI_UI=none umi dev",
    "start:prod": "cross-env REACT_APP_ENV=prod MOCK=none UMI_UI=none umi dev",
    "start:mock": "cross-env REACT_APP_ENV=mock MOCK=none UMI_UI=none umi dev",
    "test": "umi test",
    "test:all": "node ./tests/run-tests.js",
    "test:component": "umi test ./src/components",
    "tsc": "tsc",
    "g": "dip rake graphql:schema:idl && graphql-codegen --config codegen.yml"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@ant-design/icons": "^4.0.0",
    "@ant-design/pro-layout": "^6.4.9",
    "@umijs/core": "^3.1.1",
    "@umijs/plugin-access": "2.3.1",
    "@umijs/plugin-antd": "0.5.0",
    "@umijs/plugin-dva": "0.12.1",
    "@umijs/plugin-initial-state": "2.2.1",
    "@umijs/plugin-model": "2.2.0",
    "@umijs/plugin-request": "2.3.2",
    "@umijs/plugin-test": "^1.0.0",
    "date-fns": "2.18.0",
    "lodash": "^4.17.11",
    "lodash.debounce": "^4.0.8",
    "numeral": "^2.0.6",
    "omit.js": "^1.0.2",
    "path-to-regexp": "2.4.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "umi": "^3.5.0-beta.20"
  },
  "devDependencies": {
    "@types/history": "^4.7.8",
    "@types/lodash": "^4.14.168",
    "@types/lodash.debounce": "^4.0.6",
    "@types/lodash.isequal": "^4.5.5",
    "@types/numeral": "^2.0.0",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@umijs/fabric": "^2.6.2",
    "chalk": "^4.1.0",
    "cross-env": "^7.0.3",
    "cross-port-killer": "^1.3.0",
    "eslint": "^6.8.0",
    "eslint-plugin-eslint-comments": "^3.1.2",
    "eslint-plugin-jest": "^23.9.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.0.0",
    "eslint-plugin-unicorn": "^19.0.1",
    "jsdom-global": "^3.0.2",
    "prettier": "^2.2.1",
    "typescript": "^4.2.2"
  },
  "engines": {
    "node": ">=15.0.1"
  },
  "checkFiles": [
    "src/**/*.js*",
    "src/**/*.ts*",
    "src/**/*.less",
    "config/**/*.js*",
    "scripts/**/*.js"
  ]
}
